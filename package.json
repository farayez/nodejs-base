{
    "name": "nodejs-base",
    "version": "1.0.0",
    "main": "api-server.js",
    "license": "MIT",
    "type": "module",
    "scripts": {
        "prettify": "prettier -l --write \"**/*.js\"",
        "test": "npm run test:basic",
        "test:basic": "NODE_ENV=test node --experimental-vm-modules node_modules/jest/bin/jest.js --config config/jest.config.js",
        "test:coverage": "npm run test:basic -- --coverage --maxWorkers=10",
        "test:watch": "npm run test:basic -- --watchAll",
        "dev": "nodemon --inspect=0.0.0.0:9229 -L src/api-server.js",
        "model:generate": "npx sequelize-cli model:generate --name=$npm_config_name --attributes=$npm_config_attributes",
        "migrate:generate": "./node_modules/.bin/sequelize migration:create --name=$npm_config_name",
        "migrate": "npx sequelize-cli db:migrate",
        "migrate:undo": "npx sequelize-cli db:migrate:undo",
        "seed:generate": "npx sequelize-cli seed:generate --name=$npm_config_name",
        "seed": "npx sequelize-cli db:seed:all",
        "seed:undo": "npx sequelize-cli db:seed:undo"
    },
    "dependencies": {
        "@auth0/nextjs-auth0": "^3.5.0",
        "cors": "^2.8.5",
        "dotenv": "^16.3.1",
        "ejs": "^3.1.9",
        "express": "^4.18.2",
        "express-async-errors": "^3.1.1",
        "express-jwt": "^8.0",
        "express-session": "^1.17.3",
        "helmet": "^7.1.0",
        "jwks-rsa": "^3.1.0",
        "morgan": "^1.10.0",
        "passport": "^0.7.0",
        "passport-google-oauth": "^2.0.0",
        "pg": "^8.11.2",
        "sequelize": "^6.36.0",
        "sqlite3": "^5.1.2",
        "uuid": "^9.0.0",
        "wait-port": "^1.0.4"
    },
    "resolutions": {
        "ansi-regex": "5.0.1"
    },
    "devDependencies": {
        "@babel/core": "^7.23.0",
        "@babel/plugin-transform-modules-commonjs": "^7.23.3",
        "@jest/globals": "^29.7.0",
        "identity-obj-proxy": "^3.0.0",
        "jest": "^29.7.0",
        "nodemon": "^3.0.1",
        "sequelize-cli": "^6.6.2",
        "supertest": "^6.3.4"
    },
    "imports": {
        "#storage/*": "./storage/*",
        "#controllers/*": "./src/controllers/*",
        "#models/*": "./src/models/*",
        "#persistence/*": "./src/persistence/*",
        "#config/*": "./config/*"
    },
    "babel": {
        "env": {
            "test": {
                "plugins": [
                    "@babel/plugin-transform-modules-commonjs"
                ]
            }
        }
    },
    "prettier": {
        "trailingComma": "all",
        "tabWidth": 4,
        "useTabs": false,
        "semi": true,
        "singleQuote": true
    }
}
